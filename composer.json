{
  "name": "craftyfm/filemaker-proxy",
  "type": "craft-plugin",
  "license": "proprietary",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.0.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "craftyfm\\filemakerproxy\\": "src/"
    }
  },
  "extra": {
    "handle": "filemaker-proxy",
    "name": "Filemaker Proxy",
    "developer": "craftyfm",
    "documentationUrl": "",
    "class": "craftyfm\\filemakerproxy\\FmProxy"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}